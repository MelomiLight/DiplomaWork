{
    "openapi": "3.0.0",
    "info": {
        "title": "Structure API",
        "version": "1.0.0"
    },
    "paths": {
        "/api": {},
        "/api/register": {
            "post": {
                "tags": [
                    "Авторизация"
                ],
                "summary": "Регистрация",
                "description": "Get a JWT via given credentials.",
                "operationId": "ca7c0cb6772de9bffbb8c896980cebc8",
                "parameters": [
                    {
                        "name": "Accept",
                        "in": "header",
                        "required": true,
                        "example": "application/json"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "user"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "user230@crocos.kz"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "type": "string",
                                            "example": "your_token"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Could not create user"
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Авторизация"
                ],
                "summary": "Логин",
                "description": "Get a JWT via given credentials.",
                "operationId": "c373bc4cffa4a56fc64e3979b8ee7b96",
                "parameters": [
                    {
                        "name": "Accept",
                        "in": "header",
                        "required": true,
                        "example": "application/json"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "user230@crocos.kz"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "string",
                                                "example": "user_info"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/forgot": {
            "post": {
                "tags": [
                    "Авторизация"
                ],
                "summary": "Забыл пароль",
                "operationId": "ea1a70c23193e0d3eb9113fcba2e1575",
                "parameters": [
                    {
                        "name": "Accept",
                        "in": "header",
                        "required": true,
                        "example": "application/json"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "user230@crocos.kz"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "We have emailed your password reset link."
                    },
                    "500": {
                        "description": "Could not send mail or could not create reset code"
                    }
                }
            }
        },
        "/api/password/reset": {
            "post": {
                "tags": [
                    "Авторизация"
                ],
                "summary": "Поменять пароль",
                "operationId": "f3732be9b471fb1d0f6149c860ae5356",
                "parameters": [
                    {
                        "name": "Accept",
                        "in": "header",
                        "required": true,
                        "example": "application/json"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password",
                                    "reset_code"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "user230@crocos.kz"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    },
                                    "reset_code": {
                                        "type": "string",
                                        "example": "f28adw"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Your password has been reset."
                    },
                    "500": {
                        "description": "Could not change password"
                    }
                }
            }
        },
        "/api/logout": {
            "post": {
                "tags": [
                    "Авторизация"
                ],
                "summary": "Поменять пароль",
                "operationId": "5ff2dc8b48bff52908d3821a0ecc9b3e",
                "parameters": [
                    {
                        "name": "Accept",
                        "in": "header",
                        "required": true,
                        "example": "application/json"
                    },
                    {
                        "name": "Authorization",
                        "in": "header",
                        "required": true,
                        "example": "Bearer your_token"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully logged out"
                    },
                    "404": {
                        "description": "error message"
                    }
                }
            }
        }
    },
    "components": {
        "responses": {
            "401": {
                "description": "These credentials do not match our records."
            },
            "500": {
                "description": "Could not create token"
            }
        },
        "securitySchemes": {
            "sanctum": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header"
            }
        }
    }
}